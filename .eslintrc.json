{
    "parserOptions": {
      "ecmaVersion": 2020
    },
    "env": {
      "commonjs": true,
      "es2021": true,
      "node": true
    },
    "extends": ["eslint:recommended", "standard", "prettier"],
    "plugins": ["prettier"],
    "rules": {
      "prettier/prettier": "error",
      "no-console": "off",
      "no-undef": "off"
    }
  }
  